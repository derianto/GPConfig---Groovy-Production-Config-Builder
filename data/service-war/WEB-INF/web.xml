<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Data Service servlet</display-name>
  <description>Data Service servlet</description>

  <listener>
    <listener-class>org.gpconfig.fx.GpconfigServletContextListener</listener-class>
  </listener>

  <servlet>
    <servlet-name>DataServiceServlet</servlet-name>
    <servlet-class>org.dk.gpconfig.data.service.DataServiceServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DataServiceServlet</servlet-name>
    <url-pattern>/dataservice</url-pattern>
  </servlet-mapping>

</web-app>
